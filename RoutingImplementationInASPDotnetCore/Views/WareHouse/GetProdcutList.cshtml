@model IEnumerable<Product>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@foreach (var prod in Model)
{
    <div>
        <label>Product Id : @prod.ProductId </label>
        <label>Product Name : @prod.ProductName </label>
        <label>Product Description : @prod.ProductDescription </label>
        <label>Product StockCount : @prod.StockCount </label>
        <label>Product IsInStock : @prod.IsInStock </label>
    </div>

}